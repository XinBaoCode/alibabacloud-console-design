import getRoutes from '@alicloud/xconsole/lib'
import sidebar from '~/sidebar'
<% if(global.hasLayout){ %>
import AppLayout from '~/layout';
<% } else { %>
import { AppLayout } from '@alicloud/xconsole'
<% } %>
<% if(global.hasAppConfig){ %>
import appConfig from '~/appConfig';
<% } %>
import routeConfig from './route_config';

<% if(!global.hasAppConfig){ %>
const appConfig = {}
<% } %>

export default (config = {}) => {

  return getRoutes({
    sidebar,
    routeConfig,
    AppLayout,
    appConfig,
    ...config
  })
};
